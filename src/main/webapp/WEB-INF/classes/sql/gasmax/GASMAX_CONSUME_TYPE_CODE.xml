<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap 
       PUBLIC 
             "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
              "http://ibatis.apache.org/dtd/sql-map-2.dtd"
 >
 
 <sqlMap>

     <select id="GASMAX.ConsumeTypeCode.Select" resultClass="java.util.HashMap">
         Select 
         		Code AS consumeTypeCode
        	   , CodeName  AS consumeTypeName
		FROM $catalogName$.dbo.BA_Code
		WHERE  Gubun = 'SOBI'
		ORDER BY Code
	</select>
	
</sqlMap>